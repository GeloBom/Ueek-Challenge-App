{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/angular",
        "config": {
          "buildCommand": "ng build --configuration=production"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/ueek-challenge-app/$1", 
        "headers": {
          "Cache-Control": "s-maxage=3600, stale-while-revalidate=86400"
        }
      }
    ]
  }